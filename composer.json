{
	"name": "mangoweb/mail-queue",
	"type": "library",
	"description": "QueueMailer",
	"license": "proprietary",
	"version": "0.1",
	"require": {
		"php": "~7.1",
		"mangoweb/cli-worker": "@dev",
		"mangoweb/clock": "@dev",
		"mangoweb/exception-responsibility": "~0.0.1",
		"nette/mail": "~2.4"
	},
	"require-dev": {
		"mangoweb/tester-database-creator": "@dev",
		"mangoweb/log-tester": "@dev",
		"mangoweb/mail-tester": "@dev",
		"mangoweb/tester-infrastructure": "@dev",
		"mangoweb/nette-di-scope": "@dev",
		"nette/tester": "~2.0",
		"nextras/dbal": "~3.0",
		"nextras/migrations": "~3.1@dev",
		"psr/log": "~1.0",
		"symfony/console": "~4.0",
		"tracy/tracy": "~2.4",
		"phpstan/phpstan-shim": "~0.9"
	},
	"autoload": {
		"psr-4": {
			"Mangoweb\\MailQueue\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/inc"
		]
	},
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true,
		"classmap-authoritative": true,
		"bin-compat": "full"
	}
}
